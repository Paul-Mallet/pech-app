{
  "expo": {
    "name": "Pêch'App",
    "slug": "pech-app-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoSplash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LogoSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e9d99"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Pêch'App a besoin d'accéder à votre Caméra."
      },
      "bundleIdentifier": "com.students42lehavre.pechappmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoAdaptive.png",
        "backgroundColor": "#2e9d99"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.students42lehavre.pechappmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermissionText": "Pêch'App a besoin d'accéder à votre Caméra."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e3a4d02f-f1c4-40be-b640-af41fd236652"
      }
    }
  }
}
