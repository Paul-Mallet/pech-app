{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": [
      "ES2015",
      "DOM"
    ],
    "strict": true,
    "jsx": "react-native",
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/react-native-vector-icons"
    ],
    "types": [
      "react",
      "react-native",
      "react-native-vector-icons",
      "react-navigation",
      "@react-navigation/native"
    ]
  },
  "moduleResolution": "bundler"
}
