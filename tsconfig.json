{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/react-native-vector-icons"
    ],
    "types": [
      "react",
      "react-native",
      "react-native-vector-icons"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true
  },
  "extends": "expo/tsconfig.base"
}
