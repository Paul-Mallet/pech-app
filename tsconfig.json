{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "typeRoots": ["node_modules/@types", "node_modules/react-native-vector-icons"],
    "types": ["react", "react-native", "react-native-vector-icons"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}